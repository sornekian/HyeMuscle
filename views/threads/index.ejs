<%- include('../partials/header') %>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Title</th>
      <th>Content</th>
      <th>Date Posted</th>
      <th>Rev On</th>
    </tr>
  </thead>
  
  <tbody>
 
    <% threads.forEach(function(t) { %>
        <tr>
          <td><%= t.userName %></td>
          <td><%= t.title %></td>
          <td><%= t.content %></td>
          <td><%= t.createdAt.toLocaleDateString() %></td>
          <td><a href="/threads/<%= t._id %>">View Discussion</a></td>
        </tr>
        <% }); %>

  </tbody>
</table>

<%- include('../partials/footer') %>